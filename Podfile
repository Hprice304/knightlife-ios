project 'Glancer.xcodeproj'

source 'https://cdh-dev@bitbucket.org/cdh-dev/specs.git'
source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!

platform :ios, '10.0'

def glancer_pods
  pod 'AddictiveLib'
  pod 'Hero'
  pod 'SnapKit'
end

target 'Glancer' do
  glancer_pods
end

target 'TodayWidget' do
  glancer_pods
end
